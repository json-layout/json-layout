{
  "name": "@json-layout/examples",
  "version": "0.0.0",
  "description": "Examples to be used by implementations for documentation and testing.",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/cjs/index.js",
        "types": "./dist/cjs/index.d.ts"
      },
      "import": {
        "default": "./dist/mjs/index.js",
        "types": "./dist/mjs/index.d.ts"
      }
    }
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "run-s clean build:cjs build:mjs",
    "clean": "rm -rf ./dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.mjs.json",
    "lint": "eslint . --fix",
    "dev": "tsc -p tsconfig.cjs.json --watch --preserveWatchOutput"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/json-layout/json-layout.git"
  },
  "keywords": [
    "json",
    "schema",
    "forms",
    "layout"
  ],
  "author": "Alban Mouton <alban.mouton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/json-layout/json-layout/issues"
  },
  "homepage": "https://github.com/json-layout/json-layout#readme",
  "peerDependencies": {
    "@json-layout/core": "0.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "eslint": "^8.49.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.5"
  }
}
